{"_id":"sSs3hSzkKBMNBgTs","name":"Shield","permission":{"default":0},"data":{"description":{"type":"String","label":"Description","value":"<p>A shield is made from wood or metal and is carried in one hand. Wielding a shield increases your Armor Class by 2. You can benefit from only one shield at a time.</p>"},"source":{"type":"String","label":"Source","value":"PHB pg. 144"},"quantity":{"type":"Number","label":"Quantity","value":1},"weight":{"type":"Number","label":"Weight","value":6},"price":{"type":"String","label":"Price","value":"10"},"armor":{"type":"Number","label":"Armor Value","value":2},"armorType":{"type":"String","label":"Armor Type","value":"shield"},"strength":{"type":"String","label":"Required Strength","value":""},"stealth":{"type":"Boolean","label":"Stealth Disadvantage","value":false},"proficient":{"type":"Boolean","label":"Proficient","value":false},"attuned":{"type":"Boolean","label":"Attuned","value":false},"equipped":{"type":"Boolean","label":"Equipped","value":false}},"flags":{},"type":"equipment","img":"systems/dnd5e/icons/items/armor/shield.png"}
